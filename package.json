    "@types/node-telegram-bot-api": "^0.64.6",
    "@typescript-eslint/eslint-plugin": "~7.6",
    "@typescript-eslint/parser": "~7.6",
    "eslint": "~8.56",
    "eslint-config-prettier": "~9.1",
    "eslint-plugin-jest": "~28.2",
    "jest": "~29.7",
    "prettier": "~3.2",
    "rimraf": "~5.0",
    "ts-api-utils": "~1.3",
    "ts-jest": "~29.1",
    "typescript": "~5.4"
  },
  "scripts": {
