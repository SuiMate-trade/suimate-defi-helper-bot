{
  "name": "suimate-defi-helper-bot",
  "version": "0.0.0",
  "description": "Telegram bot for all things in Sui DeFi",
  "type": "module",
  "engines": {
    "node": ">= 20.9 < 21"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "~29.5",
